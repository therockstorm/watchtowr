{
  "name": "watchtowr-console",
  "version": "0.0.1",
  "private": true,
  "config": {
    "region": "us-west-2"
  },
  "scripts": {
    "build": "react-scripts build",
    "clean": "rm -rf build",
    "deploy": "aws s3 sync build/ s3://www.watchtowr.io/",
    "deploy:stack": "babel-node scripts/deployStack.js ${npm_package_name} ${npm_package_config_region}",
    "lint": "eslint src/ --fix",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test --env=jsdom",
    "validate": "babel-node scripts/validateStack.js ${npm_package_config_region}"
  },
  "dependencies": {
    "graphiql": "^0.8.1",
    "graphql": "^0.8.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "aws-sdk": "^2.7.20",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "react-scripts": "0.8.4"
  }
}
